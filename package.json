{
  "name": "@domeadev/reducer-slice",
  "version": "0.0.1",
  "description": "A lightweight utility for creating Redux-like reducer slices",
  "types": ".dist/index.d.ts",
  "main": ".dist/index.js",
  "module": ".dist/index.mjs",
  "files": [
    ".dist"
  ],
  "exports": {
    ".": {
      "import": "./.dist/index.mjs",
      "require": "./.dist/index.js"
    }
  },
  "keywords": [
    "react",
    "hook",
    "reducer",
    "useReducer",
    "utilities"
  ],
  "author": "domeafavour",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/domeafavour/reducer-slice"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest",
    "build": "tsup"
  }
}
